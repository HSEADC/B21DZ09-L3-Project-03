<h1>Feedbacks</h1>
<% if notice.present? %>
  <%= content_tag(:p, notice, class: 'inline-block px-3 py-2 mb-5 font-medium bg-slate-700', id: 'notice') %>
<% end %><%= link_to "New feedback", new_admin_feedback_path %>
<table role="grid">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">E-mail</th>
      <th scope="col">Date</th>
      <th scope="col">Action</th>
    </tr>
  </thead>

</table>
